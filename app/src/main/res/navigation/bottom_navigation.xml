<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/rankFragment"
        android:name="com.unwiringapps.earningnapp.Fragment.RankFragment"
        android:label="fragment_rank"
        tools:layout="@layout/fragment_rank" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.unwiringapps.earningnapp.Fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_allMessagesFragment"
            app:destination="@id/allMessagesFragment" />
    </fragment>
    <fragment
        android:id="@+id/inviteFragment"
        android:name="com.unwiringapps.earningnapp.Fragment.InviteFragment"
        android:label="fragment_invite"
        tools:layout="@layout/fragment_invite" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.unwiringapps.earningnapp.Fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/allMessagesFragment"
        android:name="com.unwiringapps.earningnapp.Fragment.AllMessagesFragment"
        android:label="fragment_all_messages"
        tools:layout="@layout/fragment_all_messages" />
</navigation>